            "homepage": "https://ika.ink",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/fusioncms/fusioncms/issues",
        "source": "https://github.com/fusioncms/fusioncms",
        "docs": "https://beta.getfusioncms.com/docs",
        "rss": "https://beta.getfusioncms.com/releases.json",
        "chat": "https://beta.getfusioncms.com/discord"
    },
    "require": {
        "php": "^7.2.5",
        "fusioncms/cms": "^6.0.0-beta.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": ["tests/", "fusion/tests/"]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
