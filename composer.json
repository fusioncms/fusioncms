{
    "name": "fusioncms/fusioncms",
    "description": "Create bespoke website experiences.",
    "type": "project",
    "keywords": [
        "cms",
        "fusioncms",
        "laravel"
    ],
    "homepage": "https://beta.getfusioncms.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shea \"Kai\" Lewis",
            "email": "shea@efelle.com",
            "homepage": "https://kaidesu.com",
            "role": "Lead Developer"
        },

        {
            "name": "Shane Krolikowski",
            "email": "sk@efelle.com",
            "homepage": "https://shanekrolikowski.com",
            "role": "Developer"
        },

        {
            "name": "Robert Harvey",
            "email": "robert@efelle.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/fusioncms/fusioncms/issues",
        "source": "https://github.com/fusioncms/fusioncms",
        "docs": "https://beta.getfusioncms.com/docs",
        "rss": "https://beta.getfusioncms.com/releases.json",
        "chat": "https://beta.getfusioncms.com/discord"
    },
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "caffeinated/bonsai": "^4.1",
        "caffeinated/flash": "^3.0",
        "caffeinated/menus": "^4.0",
        "caffeinated/modules": "^6.0",
        "caffeinated/shinobi": "^5.0",
        "caffeinated/themes": "^7.0",
        "composer/semver": "^1.5",
        "doctrine/dbal": "^2.9",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "laravel/passport": "^7.3",
        "laravel/scout": "^7.1",
        "symfony/yaml": "^4.3",
        "predis/predis": "^1.1",
        "barryvdh/laravel-cors": "^0.11",
        "spatie/laravel-activitylog": "^3.6",
        "spatie/laravel-analytics": "^3.7",
        "league/glide": "^1.5",
        "league/glide-laravel": "^1.0",
        "laravel/vapor-cli": "^1.1",
        "laravel/vapor-core": "^2.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-backup": "^6.7"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.7",
        "laravel/telescope": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "^4.3",
        "symfony/dom-crawler": "^4.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Themes\\": "themes/"
        },
        "files": [
            "helpers/app.php",
            "helpers/datetime.php",
            "helpers/fields.php",
            "helpers/forms.php",
            "helpers/integers.php",
            "helpers/matrix.php",
            "helpers/misc.php",
            "helpers/paths.php",
            "helpers/relationships.php",
            "helpers/seo.php",
            "helpers/settings.php",
            "helpers/strings.php",
            "helpers/themes.php",
            "helpers/upkeep.php",
            "helpers/users.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "caffeinated/shinobi"
            ]
        }
    }
}
